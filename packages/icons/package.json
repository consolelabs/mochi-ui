{
  "name": "@consolelabs/icons",
  "version": "0.0.2",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast -- --watch",
    "lint": "eslint src/ --cache",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "generate-icons": "node ./scripts/generate-icon-components.js && eslint --fix src/components/",
    "postinstall": "pnpm generate-icons"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.2.28",
    "eslint-config-custom": "workspace:*",
    "glob": "^10.3.10",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
